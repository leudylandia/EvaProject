@model PaisViewModel
@{
    ViewData["Title"] = "Eliminar";
}

<h2><icon class="glyphicon glyphicon-remove-circle"></icon>Estas seguro que deseas eliminar a <k>@Model.Pais.Name</k></h2>

<form asp-action="Delete" asp-controller="Pais" method="post">
    <input type="hidden" asp-for="Pais.Id"  />
    <input type="submit"  value="Eliminar" class="btn btn-danger" />
</form>

